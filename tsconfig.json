{
    "compilerOptions": {
        "outDir": "lib",
        "moduleResolution": "node",
        "module": "commonjs",
        "target": "es2019",
        "strict": true,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false
    },
    "exclude": [
		"node_modules",
		"lib"
	],
	"include": [
        "src/**/*"
    ]
}
